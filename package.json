{
  "name": "spifa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:client:generate": "npm exec prisma generate",
    "prisma:db:push": "npm exec prisma db push",
    "prisma:db:pull": "npm exec prisma db pull",
    "prisma:db:seed": "npx prisma db seed",
    "prisma:initdb": "npm run prisma:migrate --name create_db && npm run prisma:db:seed",
    "prisma:db:dev": "npm run prisma:db:push && npm run prisma:db:seed",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/nextjs-registry": "^1.0.1",
    "@auth/prisma-adapter": "^2.7.0",
    "@prisma/client": "^5.20.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/react-redux": "^7.1.34",
    "antd": "^5.21.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "next": "14.2.14",
    "next-auth": "^5.0.0-beta.22",
    "pg": "^8.13.0",
    "react": "^18",
    "react-dom": "^18",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
